<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke" verbose="1" >
    <groups>
        <run>
            <include name="Smoke"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.commercedemo.nop.listeners.Listeners"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
    </listeners>
    <test name="Login Test" >
        <classes>
            <class name="com.commercedemo.nop.testsuit.LoginTestCase"/>
        </classes>
    </test>
    <test name="Register Test" >
        <classes>
            <class name="com.commercedemo.nop.testsuit.RegisterTestCase"/>
        </classes>
    </test>
    <test name="Computer Test" >
        <classes>
            <class name="com.commercedemo.nop.testsuit.ComputerTestCase"/>
        </classes>
    </test>
</suite>